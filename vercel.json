{
  "version": 2,
  "builds": [
    {
      "src": "src/app/api/shopify/route.ts",
      "use": "@vercel/node"
    },
    {
      "src": "src/app/page.tsx",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/shopify",
      "dest": "/src/app/api/shopify/route.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "SHOPIFY_API_KEY": "${SHOPIFY_API_KEY}",
    "SHOPIFY_API_SECRET": "${SHOPIFY_API_SECRET}",
    "HOST": "${HOST}",
    "SCOPES": "${SCOPES}",
    "SHOPIFY_SHOP": "${SHOPIFY_SHOP}",
    "SHOPIFY_PASSWORD": "${SHOPIFY_PASSWORD}"
  }
}
